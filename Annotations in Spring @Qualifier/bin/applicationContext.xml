<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<!--Add <<xmlns:context = "http://www.springframework.org/schema/context">> 
		in <beans> and <<http://www.springframework.org/schema/context>> in xsi:schemaLocation -->

	<context:annotation-config />

	<bean id="message" class="online.mrwallet.www.Message">
		<property name="message" value="Hello Spring"></property>
	</bean>
	
	<bean id="messageType1" class="online.mrwallet.www.MessageType">
	<property name="messageType" value="Text message 1"></property>
	</bean>
	
	<bean id="messageType2" class="online.mrwallet.www.MessageType">
	<property name="messageType" value="Text message 2"></property>
	</bean>


</beans>